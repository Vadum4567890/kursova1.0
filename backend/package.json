{
  "name": "car-rental-backend",
  "version": "1.0.0",
  "description": "Backend для системи прокату автомобілів",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:clean": "node scripts/kill-port.js 3000 && npm run dev",
    "kill-port": "node scripts/kill-port.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -d src/database/data-source.ts",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -d src/database/data-source.ts",
    "seed": "ts-node src/database/seed.ts",
    "download-images": "ts-node scripts/download-car-images.ts",
    "test": "jest"
  },
  "keywords": [
    "car-rental",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mui/x-date-pickers": "^8.17.0",
    "@tanstack/react-query": "^5.90.7",
    "@types/multer": "^2.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.19.24",
    "@types/semver": "^7.7.1",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.15.4",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
